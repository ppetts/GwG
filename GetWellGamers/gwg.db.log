/*C21*/SET SCHEMA PUBLIC
drop table donation if exists
drop table hospital if exists
drop table person if exists
drop table stock if exists
create table donation (donationid varchar(255) not null, organisationid varchar(255), primary key (donationid))
create table hospital (organisationid varchar(255) not null, address1 varchar(255), address2 varchar(255), address3 varchar(255), city varchar(255), county varchar(255), email varchar(255), fax varchar(255), is_pims_managed boolean not null, latitude double not null, longitude double not null, organisation_code varchar(255), organisation_name varchar(255), organisation_status varchar(255), organisation_type varchar(255), parent_name varchar(255), parentodscode varchar(255), phone varchar(255), postcode varchar(255), sector varchar(255), sub_type varchar(255), website varchar(255), primary key (organisationid))
create table person (dtype varchar(31) not null, person_id varchar(255) not null, forename varchar(255), mobilenumber varchar(255), surname varchar(255), telephonenumber varchar(255), title integer, address1 varchar(255), address2 varchar(255), address3 varchar(255), postcode varchar(255), town varchar(255), organisationid varchar(255), primary key (person_id))
create table stock (stock_id varchar(255) not null, brand varchar(255), model varchar(255), notes varchar(255), status varchar(255), donationid varchar(255), organisationid varchar(255), staffid varchar(255), primary key (stock_id))
